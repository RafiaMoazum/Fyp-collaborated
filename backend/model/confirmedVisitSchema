const mongoose = require('mongoose');

const confirmedVisitSchema = new mongoose.Schema({
  name:{  type: String  },
  email: {  type: String },
  phone: {  type: String },
  cnic: {  type: String },
  date: { type: Date},
   time: { type: String },
  hostelId: {type: String}
});

const ConfirmedVisit = mongoose.model('Confirmedvisit', confirmedVisitSchema);

module.exports = ConfirmedVisit;
